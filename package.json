    "@mui/lab": "^5.0.0-alpha.85",
    "@mui/material": "^5.8.3",
    "@mui/x-data-grid": "^5.12.1",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "next": "^12.1.6",
    "next-dark-mode": "^3.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
